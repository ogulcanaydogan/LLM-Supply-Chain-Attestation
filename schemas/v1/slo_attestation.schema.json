{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "slo_profile_id",
    "window",
    "ttft_ms_p50",
    "ttft_ms_p95",
    "tokens_per_sec_p50",
    "cost_per_1k_tokens_cap_usd",
    "error_rate_cap",
    "error_budget_remaining"
  ],
  "properties": {
    "slo_profile_id": { "type": "string" },
    "window": {
      "type": "object",
      "required": ["start", "end"],
      "properties": {
        "start": { "type": "string", "format": "date-time" },
        "end": { "type": "string", "format": "date-time" }
      }
    },
    "ttft_ms_p50": { "type": "number" },
    "ttft_ms_p95": { "type": "number" },
    "tokens_per_sec_p50": { "type": "number" },
    "cost_per_1k_tokens_cap_usd": { "type": "number" },
    "error_rate_cap": { "type": "number" },
    "error_budget_remaining": { "type": "number" },
    "observability_query_digest": { "type": "string" }
  }
}
